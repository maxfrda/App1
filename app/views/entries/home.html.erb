    <% if !user_signed_in? %>
      <%= render :partial => 'shared/logged-out' %>
    <% elsif user_signed_in? && !@user.entries.exists? %>
      <%= render :partial => 'shared/zero-entries' %>
    <% elsif @user.entries.count == 1 %>
      <%= render :partial => 'shared/one-entry' %>
    <% else %>
      <%= render :partial => 'shared/many-entries' %>
    <% end %>






